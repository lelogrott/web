<% @receitas_collection.each do |amigo, receitas| %>
  <h3><% Usuario.find(amigo.to_s.to_i).name %></h3>
  <div class="w3-row-padding w3-padding-16 w3-center" id="food">
    <% receitas.each do |receita| %>
      <div class="w3-quarter">
        <%= image_tag receita.image.url(:thumb) %>
        <h3><%= link_to receita.title, receita_path(receita) %></h3>
      </div>
    <% end %>
  </div>
<% end %>